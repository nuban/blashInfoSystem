<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="vip.imagin.blast.modules.meterial.dao.CaseEnterPriceDao">
    <insert id="insertEnterprice"
            parameterType="vip.imagin.blast.modules.meterial.entity.Caseenterprice" useGeneratedKeys="true"
            keyProperty="caseenterpriceid"
    >
    insert into caseenterprice(caseenterpriceid,name,address,direction,reason)
    VALUES (#{caseenterpriceid},#{name},#{address},#{direction},#{reason})
</insert>
</mapper>